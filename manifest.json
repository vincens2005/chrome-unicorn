{
  "manifest_version": 2,
  "name": "Unicorn New Tab",
  "short_name": "Unicorn",
  "description": "Replace the new tab with a random unicorn or rainbow",
  "version": "1.0.0",
  "permissions": [
    "tabs",
    "storage",
    "webNavigation",
    "*://*.google.com/*",
    "*://*.github.com/*",
    "*://*.github.io/*",
    "*://*.cloudfare.com/*",
    "*://*.googleapis.com/*",
    "*://*.cornify.com/*",
    "*://*.unsplash.com/*", 
	
    "cookies","<all_urls>"

    
  ],
  
  "icons": {
    
    "19": "icon.png"
   
  },
"sandbox":{
"pages": ["unicorn.html"]
},
  "chrome_url_overrides": {
    "newtab": "unicorn.html"
  },

  
 
  "browser_action": {
    "default_title": "Unicorn",
    "default_icon": "icon.png"
  }
}